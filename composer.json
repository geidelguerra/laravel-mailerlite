{
    "name": "geidelguerra/laravel-mailerlite",
    "description": "Laravel 5 wrapper for MailerLite PHP SDK",
    "keywords": ["laravel", "mailerlite"],
    "require": {
        "mailerlite/mailerlite-api-v2-php-sdk": "^0.2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Geidel Guerra",
            "email": "geidelguerra@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GG\\MailerLite\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GG\\MailerLite\\MailerLiteServiceProvider"
            ],
            "aliases": {
                "MailerLite": "GG\\MailerLite\\MailerLiteFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
